g","generated":{"line":57,"column":8},"source":"js/JsCheveres/traerInfoCursos.js","original":{"line":85,"column":8}},{"generated":{"line":57,"column":11},"source":"js/JsCheveres/traerInfoCursos.js","original":{"line":85,"column":0}},{"name":"loadCourses","generated":{"line":57,"column":12},"source":"js/JsCheveres/traerInfoCursos.js","original":{"line":85,"column":12}},{"generated":{"line":57,"column":23},"source":"js/JsCheveres/traerInfoCursos.js","original":{"line":85,"column":23}},{"generated":{"line":57,"column":25},"source":"js/JsCheveres/traerInfoCursos.js","original":{"line":85,"column":0}}],"sources":{"js/JsCheveres/traerInfoCursos.js":"const arregloColores = [\r\n  `primary`,\r\n  `success`,\r\n  `info`,\r\n  `warning`,\r\n  `danger`,\r\n  `secondary`,\r\n  `light`,\r\n  `dark`,\r\n];\r\n\r\n// Crea el html para una card de un curso\r\nfunction generateCourseCard(nombreCurso, gradoCurso, colorCurso) {\r\n  return `<div class=\"col-lg-3 mb-4\">\r\n            <div class=\"card bg-${colorCurso} text-white shadow\">\r\n               <div class=\"card-body\">\r\n                 ${gradoCurso}\r\n               <div class=\"text-white-50 small\">${nombreCurso}</div>\r\n               </div>\r\n           </div>\r\n         </div>`;\r\n}\r\n\r\n// Cargue todo los contenidos e ubiquelos en las cartas de los cursos\r\n// RECORDAR EL ASYNC MANO\r\nfunction loadCourses() {\r\n  // const res = await fetch('http://localhost:4000/creators');\r\n  // const creators = await res.json();\r\n  const jsonPrueba = [\r\n    {\r\n      id: 1,\r\n      nombre: `Matematicas`,\r\n      grado: `6a`,\r\n      color: arregloColores[0],\r\n    },\r\n    {\r\n      id: 2,\r\n      nombre: `Español`,\r\n      grado: `2c`,\r\n      color: arregloColores[1],\r\n    },\r\n\r\n    {\r\n      id: 3,\r\n      nombre: `Sociales`,\r\n      grado: `3a`,\r\n      color: arregloColores[2],\r\n    },\r\n\r\n    {\r\n      id: 4,\r\n      nombre: `Ética`,\r\n      grado: `4d`,\r\n      color: arregloColores[3],\r\n    },\r\n\r\n    {\r\n      id: 5,\r\n      nombre: `Física`,\r\n      grado: `5d`,\r\n      color: arregloColores[4],\r\n    },\r\n\r\n    {\r\n      id: 6,\r\n      nombre: `Biología`,\r\n      grado: `3a`,\r\n      color: arregloColores[5],\r\n    },\r\n  ];\r\n  console.log(jsonPrueba);\r\n  // Obtenga el primer elemento del documento con la clase=\"container\":\r\n  const contenedorCursos = document.querySelector(`[name=\"cursos\"]`);\r\n  console.log(contenedorCursos);\r\n  // Itere y cree las cartas y los contenedores\r\n  let rta = ``;\r\n  console.log(`antes${rta}`);\r\n  jsonPrueba.forEach(curso => {\r\n    rta += generateCourseCard(curso.nombre, curso.grado, curso.color);\r\n  });\r\n  console.log(`despues${rta}`);\r\n  contenedorCursos.innerHTML = rta;\r\n}\r\n\r\nconsole.log(loadCourses());\r\n"},"lineCount":null}},"error":null,"hash":"45799085bd155e551daf133572451134","cacheData":{"env":{}}}